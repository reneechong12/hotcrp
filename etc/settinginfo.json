    },
    {
        "name_pattern": "rf__$__required",
        "title": "Review field requirement", "title_pattern": "‘${sv rf__$__name}’ requirement",
        "type": "radio", "values": [0, 1], "storage": "member.required"
    },
    {
        "name_pattern": "rf__$__visibility",
        "title": "Review field visibility", "title_pattern": "‘${sv rf__$__name}’ visibility",
        "type": "radio", "values": ["au", "pc", "audec", "admin"],
        "storage": "member.visibility"
    },
    {
        "name_pattern": "rf__$__presence",
        "title": "Review field presence", "title_pattern": "‘${sv rf__$__name}’ presence",
        "type": "radio", "values": ["all", "custom"], "storage": "member.presence",
        "parser_class": "ReviewForm_SettingParser"
    },
    {
        "name_pattern": "rf__$__condition",
        "title": "Review field presence condition", "title_pattern": "‘${sv rf__$__name}’ presence condition",
        "type": "simplestring", "size": 30, "placeholder": "Search", "storage": "member.exists_if",
        "parser_class": "ReviewFieldCondition_SettingParser"
    },
    {   "name_pattern": "rf__$__exists_if", "alias_pattern": "rf__$__condition"   },
    {   "name_pattern": "rf__$__display_space", "internal": true   },

